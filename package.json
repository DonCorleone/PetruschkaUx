{
  "name": "kinderkultur-ux",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "netlify": "NODE_OPTIONS=--inspect netlify functions:serve",
    "config-develop": "ts-node ./scripts/setenv.ts --environment=develop",
    "config-prod": "ts-node ./scripts/setenv.ts --environment=prod",
    "start": "npm run config-develop --environment=develop && ng serve --configuration=custom",
    "build": "npm run config-prod --environment=prod && ng build --configuration=custom",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "dev:ssr": "ng run kinderkulturUx:serve-ssr",
    "serve:ssr": "node dist/kinderkulturUx/server/main.js",
    "build:ssr": "ng build && ng run kinderkulturUx:server",
    "prerender": "ng run kinderkulturUx:prerender --configuration=custom"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.4",
    "@angular/common": "^16.2.4",
    "@angular/compiler": "^16.2.4",
    "@angular/core": "^16.2.4",
    "@angular/forms": "^16.2.4",
    "@angular/platform-browser": "^16.2.4",
    "@angular/platform-browser-dynamic": "^16.2.4",
    "@angular/platform-server": "^16.2.4",
    "@angular/router": "^16.2.4",
    "@netlify/functions": "^1.2.0",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@nguniversal/express-engine": "^16.2.0",
    "@popperjs/core": "^2.10.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "codelyzer": "^6.0.1",
    "express": "^4.15.2",
    "jwt-decode": "^3.1.2",
    "masonry-layout": "^4.2.2",
    "ngx-masonry": "^14.0.1",
    "realm-web": "^1.2.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/language-service": "^16.2.0",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.7",
    "babel-loader": "^8.2.3",
    "dotenv": "^12.0.4",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "netlify-cli": "^15.11.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "typescript": "~5.1.6",
    "yargs": "^17.3.1"
  }
}
