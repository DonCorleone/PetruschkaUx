{
  "name": "kinderkultur-ux",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "netlify": "NODE_OPTIONS=--inspect netlify functions:serve",
    "config-develop": "ts-node ./scripts/setenv.ts --environment=develop",
    "config-prod": "ts-node ./scripts/setenv.ts --environment=prod",
    "start": "npm run config-develop --environment=develop && ng serve --configuration=custom",
    "build": "npm run config-prod --environment=prod && ng build --configuration=custom",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run kinderkulturUx:serve-ssr",
    "serve:ssr": "node dist/kinderkulturUx/server/main.js",
    "build:ssr": "ng build && ng run kinderkulturUx:server",
    "prerender": "npm run config-prod --environment=prod && ng run kinderkulturUx:prerender",
    "mock:server": "nodemon server/server.js --ext js,ts,json,html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/ssr": "^18.2.20",
    "@netlify/functions": "^1.4.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0-rc.1",
    "@popperjs/core": "^2.10.2",
    "@types/node": "^18.18.9",
    "angular-animations": "^0.11.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.2.0",
    "codelyzer": "^6.0.1",
    "express": "^4.18.2",
    "jwt-decode": "^3.1.2",
    "realm-web": "^1.2.0",
    "swiper": "^10.2.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/express": "^4.17.21",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "babel-loader": "^8.2.3",
    "dotenv": "^16.3.1",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.17.4",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "netlify-cli": "^16.9.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "~6.1.0",
    "typescript": "~5.5.4",
    "yargs": "^17.3.1"
  }
}
